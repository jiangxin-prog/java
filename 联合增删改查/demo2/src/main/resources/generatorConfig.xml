<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
<classPathEntry    location="F:\java\response\mysql\mysql-connector-java\8.0.21\mysql-connector-java-8.0.21.jar"/>

<context id="DB2Tables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/java?useUnicode=true&amp;characterEncoding=utf-8&amp;serverTimezone=Asia/Shanghai" userId="root" password="root">
    </jdbcConnection>

    <javaTypeResolver >
        <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.example.demo1.logindemo.model" targetProject="src\main\java">
        <property name="enableSubPackages" value="true" />
        <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mapper"  targetProject="src\main\resources">
        <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.demo1.logindemo.dao"  targetProject="src\main\java">
        <property name="enableSubPackages" value="true" />
    </javaClientGenerator>
    <!--   每次换表  -->
    <table schema="ArticleType" tableName="article_type" domainObjectName="ArticleType" >
        <generatedKey column="id" sqlStatement="MySql" identity="true"/>
    </table>

</context>
</generatorConfiguration>
